{
	"compilerOptions": {
		"outDir": "dist",
		"lib": ["ESNext"],
		"target": "ESNext",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"strict": true,
		"stripInternal": true,
		"esModuleInterop": true,
		"declaration": true,
		"sourceMap": true,
		"resolveJsonModule": false,
		"noEmit": true,
		"allowImportingTsExtensions": true,
	},
	"include": ["src"]
}
